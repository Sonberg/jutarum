<div class="medium-4 columns">
    <label class="jr--title">Uppdrag</label>
    <select class="mission" onchange={{action "filterMission"  value="target.value"}}>
      <option value=undefined selected={{eq params.mission 0}}>Alla</option>
      {{#each missions as |mission|}}
          <option value="{{mission.id}}" selected={{eq params.mission mission.id}}>{{mission.name}}</option>
      {{/each}}
    </select>
</div>

<div class="medium-4 columns">
    <label class="jr--title">Elev</label>
    <select class="user" onchange={{action "filterUser"value="target.value"}}>
      <option value=undefined selected={{eq params.user 0}}>Alla</option>
      {{#each users as |user|}}
          <option value="{{user.id}}" selected={{eq params.user user.id}}>{{user.first-name}} {{user.last-name}}</option>
      {{/each}}
    </select>

</div>

